<activity acttype="fill_in_blanks" version="1.1" client="McGraw-Hill" oae-label="ACTIVITY TYPE: FILL IN THE BLANKS">

	<!-- This activity presents the students with a block of text with blank spaces into which they must type in "right answers". -->

	<title oae-label="Title">
		<![CDATA[The Title]]>
	</title>
	<instructions oae-label="Instructions" oae-type="textarea" oae-rows="3">
		<![CDATA[<font size="12px">Type in <u>too</u> + an infinitive and a <u>for</u> phrase to complete the sentences. Use the words in parentheses.  Then check your answers.</font>]]>
	</instructions>
	<spacer oae-type="spacer">
</spacer>

	<!-- Use the following option to modify how the activity will be displayed. -->

	<fontsize oae-label="Resize Font?" oae-type="select" value="0 (Normal)" opt1="-3" opt2="-2" opt3="-1" opt4="0 (Normal)" opt5="+1" opt6="+2" opt7="+3">
</fontsize>
	<spacer oae-type="spacer">
</spacer>
	<page oae-label="Page 1">

		<!-- Write the paragraphs or sentences in the following space, using the <blank word="text"/> tag to indicate where the blank lines will go, the "word" attribute to indicate what word(s) will be dragged into that blank space. Optionally, use the "align" attribute to indicate the alignment of the word dropped into the blank space and the "alternate" attribute to display a different word on the blank line instead of the dragged text. See and test the example below. To show pop-ups use <pop hotwords="text">text that pops up here</pop>. -->

		<graphic oae-label="graphic file name (optional). Please include the file extension (.jpg or .png)" oae-type="textarea" oae-rows="1">
			<![CDATA[]]>
		</graphic>
		<audio oae-label="audio file name (optional). Please include the file extension (.mp3)" oae-type="textarea" oae-rows="1">
			<![CDATA[]]>
		</audio>
		<reader oae-label="Reading text (optional)." oae-type="textarea" oae-rows="10">
			<![CDATA[]]>
